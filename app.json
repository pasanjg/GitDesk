{
	"name": "Start on Heroku: Node.js",
	"description": "A barebones Node.js app using Express 4",
	"repository": "https://github.com/pasanjg/GitDesk-dev.git",
	"logo": "https://cdn.rawgit.com/heroku/node-js-getting-started/master/public/node.svg",
	"keywords": [
		"node",
		"express",
		"heroku"
	],
	"image": "heroku/nodejs",
	"env": {
		"REACT_APP_IS_DEV": "true",
		"REACT_APP_GH_OAUTH_URL": "https://github.com/login/oauth/access_token",
		"REACT_APP_GITHUB_OAUTH_DEV_CLIENT_ID": "",
		"REACT_APP_GITHUB_OAUTH_DEV_CLIENT_SECRET": "",
		"REACT_APP_GITHUB_OAUTH_PROD_CLIENT_ID": "",
		"REACT_APP_GITHUB_OAUTH_PROD_CLIENT_SECRET": "",
		"REACT_APP_CRYPT_SALT": "",
		"OAUTH_HOST": "github.com",
		"OAUTH_PORT": "443",
		"OAUTH_PATH": "/login/oauth/access_token",
		"OAUTH_METHOD": "POST"
	}
}