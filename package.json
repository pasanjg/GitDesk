{
  "name": "github-desktop",
  "productName": "GitHub Desktop",
  "version": "0.1.0",
  "description": "An React App for GitHub",
  "author": {
    "name": "Pasan Godamune"
  },
  "private": true,
  "homepage": "server.js",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@emotion/core": "^10.0.35",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-boost": "^0.4.9",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "html-react-parser": "^0.14.0",
    "http": "0.0.1-security",
    "jquery": "^3.5.1",
    "moment": "^2.29.0",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.9.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "3.4.3",
    "react-spinners": "^0.9.0",
    "react-tooltip": "^4.2.10",
    "use-debounce": "^5.0.1"
  },
  "scripts": {
    "start": "node server.js"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "appId": "com.jayg.github-desktop",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
