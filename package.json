{
  "name": "gitdesk",
  "productName": "GitDesk",
  "version": "1.0.0",
  "description": "An React App for GitHub",
  "author": {
    "name": "Pasan Godamune"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "appId": "com.jayg.github-desktop",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@emotion/core": "^10.0.35",
    "apollo-boost": "^0.4.9",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "html-react-parser": "^0.14.0",
    "jquery": "^3.5.1",
    "moment": "^2.29.0",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.9.0",
    "react-number-format": "^4.6.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "^4.0.3",
    "react-spinners": "^0.9.0",
    "react-tooltip": "^4.2.10",
    "sass": "^1.77.8",
    "use-debounce": "^5.0.1"
  }
}
